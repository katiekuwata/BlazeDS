<?xml version="1.0" encoding="utf-8"?>
<!--
AMFX Unit Test - 10c

Author: Peter Farland (pfarland@macromedia.com)
Last Updated: 6/27/2005

Tests anonymous objects sent by reference, amongst other complex
objects, can be parsed from an AMFX formatted stream.

 -->
<amfx ver="3" xmlns="http://www.macromedia.com/2005/amfx">
    <body>
        <object>
            <traits>
                <string>prop0</string>
                <string>prop1</string>
            </traits>
            <array length="2">
                <object>
                    <traits>
                        <string>subprop0</string>
                    </traits>
                    <date>1119647239994</date>
                </object>
                <object>
                    <traits>
                        <string>subprop0</string>
                    </traits>
                    <object>
                        <traits>
                            <string>subsubprop0</string>
                        </traits>
                        <int>1000000</int>
                    </object>
                </object>
            </array>
            <ref id="5" />
        </object>
    </body>
</amfx>