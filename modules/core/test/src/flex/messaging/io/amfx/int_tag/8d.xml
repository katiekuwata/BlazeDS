<?xml version="1.0" encoding="utf-8"?>
<!--
AMFX Unit Test - 8d

Author: Peter Farland (pfarland@macromedia.com)
Last Updated: 6/23/2005

Tests that whitespace does not interfere with parsing of
integer values in an AMFX stream.

PLEASE DO NOT REFORMAT THIS XML FILE

 -->
<amfx ver="3" xmlns="http://www.macromedia.com/2005/amfx">
    <body>
        <object>
            <traits>
                <string>prop0</string>
                <string>prop1</string>
                <string>prop2</string>
                <string>prop3</string>
                <string>prop4</string>
                <string>prop5</string>
                <string>prop6</string>
                <string>prop7</string>
                <string>prop8</string>
                <string>prop9</string>
            </traits>
            <!-- Leading spaces -->
            <int>  -1</int>

            <!-- Trailing spaces -->
            <int>2  </int>

            <!-- Both sides with spaces -->
            <int>  -3  </int>

            <!-- Leading new line -->
            <int>
4</int>
            <!-- Trailing new line -->
            <int>-5
</int>

            <!-- Both sides with new lines -->
<int>
6
</int>
            <!-- Leading tab -->
            <int>	-7</int>

            <!-- Trailing tab -->
            <int>8	</int>

            <!-- Both sides with tab -->
            <int>	-9	</int>

            <!-- Mixed -->
            <int>
	      10

            </int>

        </object>
    </body>
</amfx>